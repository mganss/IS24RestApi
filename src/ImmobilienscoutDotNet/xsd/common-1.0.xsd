            <xs:documentation xml:lang="en">Defines a continent with geoCodeIds.</xs:documentation>
            <xs:documentation xml:lang="de-DE">Definiert einen Kontinent mit Angabe der geoCodeIds.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="GeoHierarchyElement"/>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Country">
        <xs:annotation>
            <xs:documentation xml:lang="en">Defines a country with name and geoCodeIds.</xs:documentation>
            <xs:documentation xml:lang="de-DE">Definiert ein Land mit Angabe von Name und geoCodeIds.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="GeoHierarchyElement"/>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Region">
        <xs:annotation>
            <xs:documentation xml:lang="en">Defines a region with name and geoCodeId.</xs:documentation>
            <xs:documentation xml:lang="de-DE">Definiert ein Bundesland mit Angabe von Name und geoCodeIds.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="GeoHierarchyElement"/>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="City">
        <xs:annotation>
            <xs:documentation xml:lang="en">Defines a city with name and geoCodeIds.</xs:documentation>
            <xs:documentation xml:lang="de-DE">Definiert Stadt/Kreis mit Angabe von Name und geoCodeId.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="GeoHierarchyElement"/>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Quarter">
        <xs:annotation>
            <xs:documentation xml:lang="en">Defines a quarter with name and geoCodeIds.</xs:documentation>
            <xs:documentation xml:lang="de-DE">Definiert Bezirk/Gemeinde mit Angabe von Name und geoCodeIds.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="GeoHierarchyElement"/>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="Neighbourhood">
        <xs:annotation>
            <xs:documentation xml:lang="en">Defines a neighbourhood with geoCodeIds.</xs:documentation>
            <xs:documentation xml:lang="de-DE">Definiert Nachbarschaft mit Angabe der geoCodeIds.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="GeoHierarchyElement"/>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="GeoHierarchyStatistic">
        <xs:annotation>
            <xs:documentation xml:lang="en">Holds the statistics (i.e. number of objects) for a real estate type.
            </xs:documentation>
            <xs:documentation xml:lang="de-DE">Enthält die Statistik (d.h. Anzahl der Objekte) für einen
                Immobilientyp.
            </xs:documentation>
        </xs:annotation>
        <xs:attribute name="realEstateType" type="RealEstateType" use="required"/>
        <xs:attribute name="count" type="xs:int" use="required"/>
    </xs:complexType>

    <xs:complexType name="GeoHierarchyStatistics">
        <xs:annotation>
            <xs:documentation xml:lang="en">List element for GeoHierarchyStatistic.</xs:documentation>
            <xs:documentation xml:lang="de-DE">Listenelement für GeoHierarchyStatistic.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="GeoHierarchyStatistic" type="GeoHierarchyStatistic" minOccurs="0" maxOccurs="30"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="GeoHierarchyElement">
        <xs:annotation>
            <xs:documentation xml:lang="en">Defines a region with all informations.</xs:documentation>
            <xs:documentation xml:lang="de-DE">Definiert eine Region mit allen verfügbaren Informationen.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="name" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The name of the region, like a region, city or quarter.
                    </xs:documentation>
                    <xs:documentation xml:lang="de-DE">Der Name der Region, entweder ein Bundesland, Stadt oder
                        Stadtbezirk.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="geoCodeId" type="xs:long">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The IS24 GeoCode of the region.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Der IS24 GeoCode der Region.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="fullGeoCodeId" type="xs:string" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The IS24 GeoCode of the corresponding hierarchy.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Der IS24 GeoCode der jeweiligen Hierarchie.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="statistics" type="GeoHierarchyStatistics" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Optional element which holds the statistics (i.e. number of objects)
                        for a real estate type
                    </xs:documentation>
                    <xs:documentation xml:lang="de-DE">Optionales Element das die Statistik (d.h. Anzahl der Objekte)
                        für einen Immobilientyp enthält
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="GeoHierarchyElements">
        <xs:annotation>
            <xs:documentation xml:lang="en">List element for GeoHierarchyElement.</xs:documentation>
            <xs:documentation xml:lang="de-DE">Listenelement für GeoHierarchyElement.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="geoHierarchyElement" type="GeoHierarchyElement" minOccurs="0" maxOccurs="30"/>
        </xs:sequence>
    </xs:complexType>


    <xs:element name="geoHierarchyElementsStatistic">
        <xs:annotation>
            <xs:documentation xml:lang="en">List element for statistics of GeoHierarchyElements.</xs:documentation>
            <xs:documentation xml:lang="de-DE">Listenelement für Statistiken von GeoHierarchyElementen.
            </xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="parent" type="GeoHierarchyElement"/>
                <xs:element name="children" type="GeoHierarchyElements"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:group name="AssistedLivingInformation">
        <xs:annotation>
            <xs:documentation xml:lang="en">Additional Elements for the apartment types senior care and assisted living
                (not in Resultlist).
            </xs:documentation>
            <xs:documentation xml:lang="de-DE">Zusätzliche Eigenschaften für die Wohnungstypen Pflegeheim und
                Seniorenwohnen, die sich nicht in der
                Ergebnisliste befinden.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="barrierFree" type="YesNotApplicableType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">barriere free</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Barrierefrei</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="numberOfLookedAfterApartments" type="xs:int" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">number of apartments that are looked after</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Anzahl betreute Wohnungen</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="numberOfNursingPlaces" type="xs:int" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Number of the nursing places</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Anzahl Pflegeplätze</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="livingSpaceFrom" type="xs:double" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Living space to</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Wohnfläche von</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="livingSpaceTo" type="xs:double" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Living space to</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Wohnfläche bis</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="handicappedAccessible" type="YesNotApplicableType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">handicapped accessible</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Behindertengerecht</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="guestApartmentsAvailable" type="YesNotApplicableType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Guest apartments available</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Gästeappartments verfügbar</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="restaurantAvailable" type="YesNotApplicableType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Restaurant available</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Restaurant/ Café verfügbar</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="cookingFacilitiesAvailable" type="YesNotApplicableType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Cooking facilities available</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Eigene Kochmöglichkeit</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="ownFurniturePossible" type="YesNotApplicableType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Own furniture possible</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Mitnahme eigener Möbel möglich</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="cleaningServiceAvailable" type="YesNotApplicableType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Cleaning service available</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Reinigungsservice verfügbar</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="shoppingFacilitiesAvailable" type="YesNotApplicableType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Shopping facilities available</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Einkaufsmöglichkeiten im Haus</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="security24Hours" type="YesNotApplicableType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">security 24 hours</xs:documentation>
                    <xs:documentation xml:lang="de-DE">24 Stunden Sicherheit</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="culturalProgramAvailable" type="YesNotApplicableType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">cultural program available</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Kulturprogramm verfügbar</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="leisureActivitiesAvailable" type="YesNotApplicableType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">leisureActivitiesAvailable</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Freizeitaktivitäten verfügbar</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="religiousOfferingsAvailable" type="YesNotApplicableType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Religious offerings available</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Religiöse Angebote</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="balconyAvailable" type="BalconyAvailableType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Room type</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Zimmertyp</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="branchEntry" type="xs:boolean" minOccurs="0" default="false">
                <xs:annotation>
                    <xs:documentation xml:lang="en">listed in yellow pages</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Branchenbucheintrag</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:group>

    <xs:complexType name="Imprint">
        <xs:annotation>
            <xs:documentation xml:lang="en">Impressum/Imprint</xs:documentation>
            <xs:documentation xml:lang="de-DE">Impressum</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="text" type="TextField"/>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="TextField">
        <xs:annotation>
            <xs:documentation xml:lang="en">in Bezug auf XML ungültige Zeichen werden bei der Ausgabe ausgefiltert</xs:documentation>
            <xs:documentation xml:lang="de-DE">invalid characters w.r.t. XML will be filtered out on outgoing responses</xs:documentation>
            <xs:appinfo>
                <jaxb:javaType name="java.lang.String"
                               parseMethod="de.is24.common.xml.XmlSafeCharacterAdapter.unmarshal"
                               printMethod="de.is24.common.xml.XmlSafeCharacterAdapter.marshal"/>
            </xs:appinfo>
        </xs:annotation>
        <xs:restriction base="xs:string"/>
    </xs:simpleType>

</xs:schema>
