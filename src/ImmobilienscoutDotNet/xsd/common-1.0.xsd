        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="Price">
        <xs:annotation>
            <xs:documentation xml:lang="en">General type for prices.</xs:documentation>
            <xs:documentation xml:lang="de-DE">Basistyp für Preise.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="value" type="xs:double">
                <xs:annotation>
                    <xs:documentation xml:lang="en">value</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Preis</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="currency" type="Currency">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Currency of the price (generally EUR)</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Währung des Preises (i.d.R EUR)</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="marketingType" type="MarketingType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Defines the meaning of the price value. Mandatory for
                        commercial real estates.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Bestimmt die Bedeutung des Preises näher. Pflichtfeld
                        für Gewerbeimmobilien.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="priceIntervalType" type="PriceIntervalType" minOccurs="0" maxOccurs="1">
                <xs:annotation>
