<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<!--
    CHANGE WITH CARE!
    This common schema is used in all API contexts, so be very careful to not introduce incompatible changes.
    In case of doubt please contact team Curiosity or the API-interest group for support.
    -->
<xs:schema xmlns="http://rest.immobilienscout24.de/schema/common/1.0"
           xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
           jaxb:version="2.0"
           xmlns:annox="http://annox.dev.java.net"
           jaxb:extensionBindingPrefixes="annox"
           xmlns:jl="http://annox.dev.java.net/java.lang"
           xmlns:xlink="http://www.w3.org/1999/xlink"
           targetNamespace="http://rest.immobilienscout24.de/schema/common/1.0"
           elementFormDefault="unqualified"
           attributeFormDefault="unqualified"
           version="1.0">

    <xs:import namespace="http://www.w3.org/1999/xlink"/>
    <xs:include schemaLocation="includes/messages-1.0.xsd"/>

    <xs:element name="attachment" type="Attachment">
        <xs:annotation>
            <xs:documentation xml:lang="en">one attachment</xs:documentation>
            <xs:documentation xml:lang="de-DE">ein Anhang</xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:element name="realtorContactDetail" type="RealtorContactDetails">
        <xs:annotation>
            <xs:documentation xml:lang="en">The contact details for a realtor.</xs:documentation>
            <xs:documentation xml:lang="de-DE">Die Kontaktdaten eines Anbieters</xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:element name="realtorContactDetailsList">
        <xs:annotation>
            <xs:documentation xml:lang="en">List of contact details of a realtor.</xs:documentation>
            <xs:documentation xml:lang="de-DE">List von Kontaktdaten eines Anbieters</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="realtorContactDetails" type="RealtorContactDetails" minOccurs="0"
                            maxOccurs="unbounded">
                    <xs:annotation>
                        <xs:documentation xml:lang="en">The contact details for a realtor.</xs:documentation>
                        <xs:documentation xml:lang="de-DE">Die Kontaktdaten eines Anbieters</xs:documentation>
                    </xs:annotation>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="realtorLogo" type="RealtorLogo">
        <xs:annotation>
            <xs:documentation xml:lang="en">The logo of a realtor.</xs:documentation>
            <xs:documentation xml:lang="de-DE">Das Logo eines Anbieters</xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:element name="autopilotSettings" type="AutopilotSettings">
        <xs:annotation>
            <xs:documentation xml:lang="en">Autopilot configuration.</xs:documentation>
            <xs:documentation xml:lang="de-DE">Die Konfiguration des Autopiloten</xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:element name="attachments" type="Attachments">
        <xs:annotation>
            <xs:documentation xml:lang="en">List of Attachments</xs:documentation>
            <xs:documentation xml:lang="de-DE">Liste von Anhängen</xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:element name="user" type="User">
        <xs:annotation>
            <xs:documentation xml:lang="en">one user</xs:documentation>
            <xs:documentation xml:lang="de-DE">Ein Benutzer</xs:documentation>
        </xs:annotation>
    </xs:element>

    <xs:element name="strictList">
        <xs:annotation>
            <xs:documentation xml:lang="en">List of entries in strict format</xs:documentation>
            <xs:documentation xml:lang="de-DE">To be done</xs:documentation>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="paging" type="Paging" minOccurs="0"/>
                <xs:element name="matchCountList" type="MatchCountList" minOccurs="0"/>
                <xs:element name="strictEntry" type="StrictEntry" minOccurs="0" maxOccurs="unbounded"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:complexType name="StrictEntry">
        <xs:annotation>
            <xs:documentation xml:lang="en">An entry in strict format</xs:documentation>
            <xs:documentation xml:lang="de-DE">To be done</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="wgs84Coordinate" type="Wgs84Coordinate"/>
            <xs:element name="preciseHouseNumber" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">whether precision of the coordinates is of the level of house
                        number
                    </xs:documentation>
                    <xs:documentation xml:lang="de-DE">ob die Koordinaten hausnummergenau sind</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="publishedAfterRequestedDate" minOccurs="0" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation xml:lang="en">the entity was published after the requested date</xs:documentation>
                    <xs:documentation xml:lang="de-DE">die Entität nach dem Datum im Request eingestellt wurde.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>

        <xs:attributeGroup ref="Reference"/>
    </xs:complexType>

    <xs:complexType name="AbstractRealEstateForList" abstract="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">General elements for all real estates within the search context.
            </xs:documentation>
            <xs:documentation xml:lang="de-DE">Allgemeine Elemente für alle Immobilienarten.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="externalId" type="TextField" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
                        Represents a customer specific identifier for the real estate.
                        This identifier is unique in combination with the customer id.
                    </xs:documentation>
                    <xs:documentation xml:lang="de-DE">
                        Repräsentiert eine kundenspezifische Referenz einer Immobilie.
                        Der Kennzeichner muss eineindeutig in Verbindung mit der Kundennummer sein.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="title">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Heading of the offering</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Überschrift/Titel des Angebots</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="TextField">
                        <xs:maxLength value="100"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="creationDate" type="xs:dateTime" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The date of issue of the offering</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Erstelldatum des Angebots</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="lastModificationDate" type="xs:dateTime" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The date of the last modification on the offering</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Letztes Änderungsdatum</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="address" type="Wgs84Address">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The address of the real estate</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Die Adresse der Immobilie</xs:documentation>
                </xs:annotation>
            </xs:element>
            
            <xs:element name="apiSearchData" type="ApiSearchData"
                        minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Holds the information for the API search of a real estate.
                    </xs:documentation>
                    <xs:documentation xml:lang="de-DE">Enthält die Informationen für die API Suche einer Immobilie.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
			<xs:element name="realEstateState" type="RealEstateState" minOccurs="0">
		        <xs:annotation>
		            <xs:documentation xml:lang="en">Publication state of a real Estate.</xs:documentation>
		            <xs:documentation xml:lang="de-DE">Veröffentlichungsstatus einer Immobilie.</xs:documentation>
       			</xs:annotation>		
			</xs:element>            
        </xs:sequence>
        <xs:attribute name="id" type="xs:long"
                      use="optional"><!-- optional because	POST for a realestate CANNOT have an id -->
            <xs:annotation>
                <xs:documentation xml:lang="en">IS24 ID of the real Estate</xs:documentation>
                <xs:documentation xml:lang="de-DE">IS24 ID der Immobilie</xs:documentation>
            </xs:annotation>
        </xs:attribute>
    </xs:complexType>

    <xs:complexType name="AbstractRealEstate" abstract="true">
        <xs:annotation>
            <xs:documentation xml:lang="en">General elements for all real estates for the expose.</xs:documentation>
            <xs:documentation xml:lang="de-DE">Allgemeine Elemente für alle Immobilienarten im Expose.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="AbstractRealEstateForList">
                <xs:sequence>
                    <xs:element name="groupNumber" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">group number of the real estate</xs:documentation>
                            <xs:documentation xml:lang="de-DE">Gruppennummer der Immobilie</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="xs:int">
                                <xs:minInclusive value="0"/>
                                <xs:maxInclusive value="2147483647"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="descriptionNote" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Description of the real estate</xs:documentation>
                            <xs:documentation xml:lang="de-DE">Objektbeschreibung</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="TextField">
                                <xs:maxLength value="2000"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="furnishingNote" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Furnishing of the real estate</xs:documentation>
                            <xs:documentation xml:lang="de-DE">Ausstattung</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="TextField">
                                <xs:maxLength value="2000"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="locationNote" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Location information of the real estate</xs:documentation>
                            <xs:documentation xml:lang="de-DE">Lage</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="TextField">
                                <xs:maxLength value="2000"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                    <xs:element name="otherNote" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Other description of the real estate</xs:documentation>
                            <xs:documentation xml:lang="de-DE">Sonstiges</xs:documentation>
                        </xs:annotation>
                        <xs:simpleType>
                            <xs:restriction base="TextField">
                                <xs:maxLength value="2000"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>

                    <xs:element name="attachments" minOccurs="0" type="Attachments">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">Attachments of the real estate</xs:documentation>
                            <xs:documentation xml:lang="de-DE">Anhänge der Immobilie</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="ApiSearchData">
        <xs:annotation>
            <xs:documentation xml:lang="en">Holds the information for the API search of a real estate.
            </xs:documentation>
            <xs:documentation xml:lang="de-DE">Enthält die Informationen für die API Suche einer Immobilie.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="searchField1" type="searchField" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">search field 1</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Suchfeld 1</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="searchField2" type="searchField" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">search field 2</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Suchfeld 2</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="searchField3" type="searchField" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">search field 3</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Suchfeld 3</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:simpleType name="searchField">
        <xs:annotation>
            <xs:documentation xml:lang="en">Type of searchField</xs:documentation>
            <xs:documentation xml:lang="de-DE">Suchfeldtyp</xs:documentation>
        </xs:annotation>
        <xs:restriction base="TextField">
            <xs:maxLength value="16"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="depositField">
        <xs:annotation>
            <xs:documentation xml:lang="en">deposit</xs:documentation>
            <xs:documentation xml:lang="de-DE">Kaution</xs:documentation>
        </xs:annotation>
        <xs:restriction base="TextField">
            <xs:maxLength value="50"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="Wgs84Coordinate">
        <xs:annotation>
            <xs:documentation xml:lang="en">Geo information of the real estate.</xs:documentation>
            <xs:documentation xml:lang="de-DE">Geoinformationen der Immobilie.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="latitude">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Latitude (Y-Value) of the WGS84 coordinate (GPS).
                    </xs:documentation>
                    <xs:documentation xml:lang="de-DE">Latitude (Y-Wert) der WGS84-Koordinate (GPS).</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:double">
                        <xs:minInclusive value="-90.0"/>
                        <xs:maxInclusive value="90.0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="longitude">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Longitude (X-Value) of the WGS84 coordinate (GPS).
                    </xs:documentation>
                    <xs:documentation xml:lang="de-DE">Longitude (X-Wert) der WGS84-Koordinate (GPS).</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:double">
                        <xs:minInclusive value="-180.0"/>
                        <xs:maxInclusive value="180.0"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Address">
        <xs:annotation>
            <xs:documentation xml:lang="en">Definition of base address data fields.</xs:documentation>
            <xs:documentation xml:lang="de-DE">Definition der Basisfelder einer Adresse.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="street" type="TextField" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Street without house number. Mandatory for germany, optional
                        for foreign real estates.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Straße ohne Hausnummer. Pflichtfeld für Deutschland,
                        optional für Auslandsimmobilien.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="houseNumber" type="TextField" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">House number. Mandatory for germany, optional for foreign
                        real estates. Also optional for LIVING_RENT_SITE, LIVING_BUY_SITE and TRADE_SITE</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Hausnummer. Pflichtfeld für Deutschland, optional für
                        Auslandsimmobilien. Weiterhin optional für LIVING_RENT_SITE,
                        LIVING_BUY_SITE and TRADE_SITE</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="postcode" type="TextField" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">zip code</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Postleitzahl</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="city" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">city</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Stadt</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="TextField">
                        <xs:maxLength value="50"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="internationalCountryRegion" type="CountryRegion" minOccurs="0">
	                <xs:annotation>
	                    <xs:documentation xml:lang="en">Only for locations (of real estates) outside germany:
                            country and region.</xs:documentation>
	                    <xs:documentation xml:lang="de-DE">Nur für Ortsangaben (von Immobilien) außerhalb
                            Deutschlands: Land und Region.</xs:documentation>
	                </xs:annotation>		           
            </xs:element>            
        </xs:sequence>
    </xs:complexType>
	<xs:complexType name="CountryRegion">
		<xs:sequence>
			<xs:element name="country" type="CountryCode">
	                <xs:annotation>
	                    <xs:documentation xml:lang="en">The ISO-3 countrycode.</xs:documentation>
	                    <xs:documentation xml:lang="de-DE">Der ISO-3 Ländercode.</xs:documentation>
	                </xs:annotation>		
			</xs:element>
			<xs:element name="region" nillable="false">
	                <xs:annotation>
	                    <xs:documentation xml:lang="en">German name of the region (province, federal state) of the country.
                            Valid names are published (CSV-file) by is24, see online API documentation.</xs:documentation>
	                    <xs:documentation xml:lang="de-DE">Deutscher Name der Region des Landes. Mögliche Namen
                            sind über die Online-API-Dokumentation abrufbar.</xs:documentation>
	                </xs:annotation>
	                <xs:simpleType>
	                    <xs:restriction base="TextField">                       
							<xs:minLength value="1"/>	                        
	                    </xs:restriction>
	                </xs:simpleType>                		
			</xs:element>	
		</xs:sequence>
	</xs:complexType>
    <xs:complexType name="RealtorLogo">
        <xs:sequence>
            <xs:element name="realtorLogoUrl" type="xs:anyURI" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">URL to realtor's logo</xs:documentation>
                    <xs:documentation xml:lang="de-DE">URL zum Relatorlogo</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="AutopilotSettings">
        <xs:sequence>
            <xs:element name="autopilotEnabled" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">is autopilot enabled</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Ist der Autopilot aktiviert</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="sendEmailWithAutopilotReport" type="xs:boolean" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">is the autpilot email report enabled</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Ist der Autopilot Emailreport aktiviert</xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="BaseContactDetails">
        <xs:sequence>
            <xs:element name="email" type="Email" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Email of this contact.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">E-Mail des Kontakts.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="salutation" type="SalutationType" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Salutation of this contact.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Email des Kontakts.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="firstname" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Firstname of this contact.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Vorname des Kontakts.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="TextField">
                        <xs:maxLength value="30"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="lastname" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Lastname of this contact.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Nachname des Kontakts.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="TextField">
                        <xs:maxLength value="50"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="faxNumberCountryCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Facsimile country code of this contact.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Faxländervorwahl des Kontakts.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="\+[1-9]\d{0,3}"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="faxNumberAreaCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Facsimile area code of this contact.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Faxvorwahl des Kontakts.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="(:?([1-9]\d{1,9})|\(0(\d{1,9})\))"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="faxNumberSubscriber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Facsimile subscriber number of this contact.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Faxanschlussnummer des Kontakts.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="[\d][\d \-]{0,24}[\d]"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="faxNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Whole facsimile number of this contact.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Komplette Faxnummer des Kontakts.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern
                                value="(\+[1-9]\d{0,3}) +(:?([1-9]\d{1,9})|\(0(\d{1,9})\)) +([\d][\d \-]{0,24}[\d])"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>

            <xs:element name="phoneNumberCountryCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Phonenumber country code of this contact.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Telefonländervorwahl des Kontakts.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="\+[1-9]\d{0,3}"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="phoneNumberAreaCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Phonenumber area code of this contact.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Telefonvorwahl des Kontakts.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="(:?([1-9]\d{1,9})|\(0(\d{1,9})\))"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="phoneNumberSubscriber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Phonenumber subscriber number of this contact.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Telefonanschlussnummer des Kontakts.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="[\d][\d \-]{0,24}[\d]"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="phoneNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Whole phonenumber number of this contact.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Komplette Telefonnummer des Kontakts.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern
                                value="(\+[1-9]\d{0,3}) +(:?([1-9]\d{1,9})|\(0(\d{1,9})\)) +([\d][\d \-]{0,24}[\d])"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="cellPhoneNumberCountryCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Cell phonenumber country code of this contact.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Mobiltelefonländervorwahl des Kontakts.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="\+[1-9]\d{0,3}"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="cellPhoneNumberAreaCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Cell phonenumber area code of this contact.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Mobiltelefonvorwahl des Kontakts.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="(:?([1-9]\d{1,9})|\(0(\d{1,9})\))"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="cellPhoneNumberSubscriber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Cell phonenumber subscriber number of this contact.
                    </xs:documentation>
                    <xs:documentation xml:lang="de-DE">Mobilfunkanschlussnummer des Kontakts.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="[\d][\d \-]{0,24}[\d]"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="cellPhoneNumber" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">Whole cell phonenumber number of this contact.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Komplette Mobilfunknnummer des Kontakts.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern
                                value="(\+[1-9]\d{0,3}) +(:?([1-9]\d{1,9})|\(0(\d{1,9})\)) +([\d][\d \-]{0,24}[\d])"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="address" type="Address" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The base address of this contact.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Die Basisadresse des Kontakts.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="countryCode" type="CountryCode" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The ISO-3 countrycode for this contact.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Der ISO-3 Ländercode des Kontakts.</xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="title" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The contact's title.</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Der Titel des Kontakts</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="TextField">
                        <xs:maxLength value="50"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="additionName" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">addition to the name</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Namenszusatz</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="TextField">
                        <xs:maxLength value="30"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="company" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">The name of the company</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Der Name des Unternehmens</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="TextField">
                        <xs:maxLength value="100"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="homepageUrl" minOccurs="0">
                <xs:annotation>
                    <xs:documentation xml:lang="en">homepageUrl</xs:documentation>
                    <xs:documentation xml:lang="de-DE">Homepage des Anbieters</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:anyURI">
                        <xs:maxLength value="300"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element> 
            <xs:element name="portraitUrl" minOccurs="0">
               <xs:annotation>
                   <xs:documentation xml:lang="en">Url to Contacts Portrait</xs:documentation>
                   <xs:documentation xml:lang="de-DE">Url zum Bild des Anbieters</xs:documentation>
               </xs:annotation>
               <xs:simpleType>
                   <xs:restriction base="xs:anyURI">
                       <xs:maxLength value="300"/>
                   </xs:restriction>
               </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
