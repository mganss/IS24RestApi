                </xs:annotation>
                <xs:sequence>
                    <xs:element name="quarter" type="TextField" minOccurs="0">
                        <xs:annotation>
                            <xs:documentation xml:lang="en">District or quarter of this address.</xs:documentation>
                            <xs:documentation xml:lang="de-DE">Bezirk oder Stadtteil der Adresse.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="wgs84Coordinate" type="Wgs84Coordinate" minOccurs="0"/>
                    <xs:element name="preciseHouseNumber" type="xs:boolean" minOccurs="0">
